<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="DeveloperApiTimeLog">

    <resultMap id="resultMap-DeveloperApiTimeLog" type="API">
        <result property="createTime" column="create_time"/>
        <result property="apiId" column="api_id"/>
        <result property="developerId" column="developer_id"/>
    </resultMap>

    <insert id="save" parameterType="DeveloperApiTimeLog" keyProperty="id">
        <![CDATA[
        INSERT INTO developer_api_time_log(api_id,developer_id,create_time,time)
        VALUES(#{apiId},#{developerId},now(),${time})
        ]]>
    </insert>

    <insert id="saveBatch" parameterType="java.util.List">
        <![CDATA[
        INSERT INTO developer_api_time_log(api_id,developer_id,create_time,time)
        VALUES
        ]]>
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.apiId},#{item.developerId},now(),${item.time})
        </foreach>
    </insert>

</mapper>
